{
  "name": "rpage",
  "version": "1.0.0",
  "description": "A simple RPA Application",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "server": "node --no-warnings server.cjs",
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "test": "npx vitest",
    "lint": "eslint .",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "rpa",
    "playwright",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "playwright": "^1.50.1",
    "svelte": "^4.2.11"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/svelte": "^4.0.5",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "eslint": "^8.54.0",
    "eslint-plugin-svelte": "^2.35.0",
    "express": "^4.21.2",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.8.2",
    "vite": "^5.4.14",
    "vitest": "^1.6.1"
  }
}
